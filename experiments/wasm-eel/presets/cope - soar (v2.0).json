 = uv.y;\n  ret_1 = (texture (sampler_main, tmpvar_2).xyz + texture (sampler_main, uv).xyz);\n  ret_1 = (ret_1 * 1.42);\n  ret_1 = (ret_1 * ret_1);\n  vec4 tmpvar_3;\n  tmpvar_3.w = 1.0;\n  tmpvar_3.xyz = ret_1;\n  ret = tmpvar_3.xyz;\n }","warp_hlsl":"shader_body\n{\n    ret = tex2D( sampler_fc_main, uv ).xyz;\n    \n    ret *= 0.95; \n}","comp_hlsl":"sampler sampler_sky;\nshader_body\n{\n    ret = tex2D(sampler_main, float2(1-uv.x, uv.y)).xyz;\n    ret += tex2D(sampler_main, uv).xyz;\n    //ret += tex2D(sampler_sky, float2(uv.x, uv.y-q6*0.5))*(1-uv.y);\n    //ret *= (GetBlur3(uv)+GetBlur3(float2(1-uv.x, uv.y)))*1; \n    ret *= 1.42; //gamma\n \n    //ret = sqrt(ret); //brighten\n    ret *= ret; //darken\n}"}